<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.atomicoke.logic.modules.friend.domain.dao.mapper.FriendRequestMapper">

    <resultMap id="BaseResultMap" type="org.atomicoke.logic.modules.friend.domain.entity.FriendRequest">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="applyId" column="apply_id" jdbcType="BIGINT"/>
        <result property="userId" column="user_id" jdbcType="BIGINT"/>
        <result property="applyMessage" column="apply_message" jdbcType="VARCHAR"/>
        <result property="addWay" column="add_way" jdbcType="INTEGER"/>
        <result property="handlerResult" column="handler_result" jdbcType="INTEGER"/>
        <result property="handlerTime" column="handler_time" jdbcType="TIMESTAMP"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="randomId" column="random_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,apply_id,user_id,
        apply_message,add_way,handler_result,
        handler_time,create_time, random_id
    </sql>
    <insert id="insertIgnore">
        insert ignore into friend_request
        (<include refid="Base_Column_List"/>)
        values
        (#{id},#{applyId},#{userId},#{applyMessage},
        #{addWay},#{handlerResult},#{handlerTime},
        #{createTime},#{randomId});
    </insert>
</mapper>